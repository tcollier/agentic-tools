{
  "name": "agentic-tools",
  "version": "1.0.0",
  "description": "Claude Code command for stage-appropriate codebase auditing",
  "private": true,
  "scripts": {
    "test": "promptfoo eval",
    "test:ui": "promptfoo eval && promptfoo view",
    "test:watch": "promptfoo eval --watch",
    "test:filter": "promptfoo eval --filter",
    "test:verbose": "promptfoo eval --verbose",
    "view": "promptfoo view"
  },
  "keywords": [
    "claude",
    "ai",
    "prompts",
    "testing",
    "development"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "promptfoo": "^0.119.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
